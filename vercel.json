{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/dist/assets/$1"
    },
    {
      "source": "/vite.svg",
      "destination": "/dist/vite.svg"
    },
    {
      "source": "/((?!api/|uploads/|assets/).*)",
      "destination": "/dist/index.html"
    }
  ]
}